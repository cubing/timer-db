{
  "name": "timer-db",
  "version": "0.0.6",
  "description": "A library for cubing timer results storage with CouchDB-based synchronization.",
  "type": "module",
  "types": "dist/types/timer-db.d.ts",
  "main": "dist/esm/index.js",
  "browser-global": "dist/browser-global/timer-db.browser-global.js",
  "scripts": {
    "build": "node script/build.js && npm run build-types",
    "build-types": "npx tsc --build tsconfig-types.json",
    "dev": "node script/dev.js",
    "clean": "rm -rf ./.parcel-cache ./dist",
    "prepack": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cubing/timer-db"
  },
  "keywords": [
    "cubing"
  ],
  "author": "github.com/cubing",
  "license": "GPL-3.0-or-later",
  "targets": {
    "types": {},
    "main": {
      "context": "node",
      "isLibrary": true,
      "outputFormat": "commonjs",
      "minify": false
    },
    "module": {
      "isLibrary": true,
      "outputFormat": "esmodule",
      "minify": false
    },
    "browser-global": {
      "context": "browser",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": true
    }
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/pouchdb": "^6.4.0",
    "barely-a-dev-server": "^0.2.5",
    "esbuild": "^0.14.5",
    "pouchdb": "^7.2.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@collectable/red-black-tree": "^5.0.1",
    "events": "^3.3.0",
    "pouchdb-find": "^7.2.1"
  }
}
